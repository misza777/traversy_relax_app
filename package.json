{
  "name": "mishiko-portfolio-webpack",
  "version": "1.0.0",
  "author": "Misza777",
  "description": "Misza777 webpack grid portfolio with HMR, sass, jquery",
  "repository": {
    "type": "git",
    "url": "https://github.com/https://github.com/misza777/Misza777_Webpack_Template"
  },
  "main": "index.js",
  "scripts": {
    "start": "cross-env NODE_ENV=development webpack-dev-server --inline --progress --colors --open --config ./webpack.config.dev.js",
    "build": "cross-env NODE_ENV=production webpack --config ./webpack.config.prod.js",
    "dev": "cross-env NODE_ENV=development webpack --config ./webpack.config.dev.js",
    "dev:watch": "npm run dev -- --watch",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build"
  },
  "keywords": [
    "webpack",
    "boilerplate",
    "hmr",
    "sass"
  ],
  "license": "ISC",
  "browserslist": "> 0.2%",
  "devDependencies": {
    "@babel/core": "^7.8.4",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/preset-env": "^7.8.4",
    "@babel/preset-es2015": "^7.0.0-beta.53",
    "@babel/preset-react": "^7.8.3",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-loader": "^8.0.6",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-preset-react-hmre": "^1.1.1",
    "clean-webpack-plugin": "^3.0.0",
    "copy-webpack-plugin": "^5.1.1",
    "core-js": "^3.6.4",
    "cross-env": "^6.0.0",
    "css-hot-loader": "^1.4.4",
    "css-loader": "^3.2.0",
    "html-loader": "^1.1.0",
    "html-webpack-plugin": "^3.2.0",
    "mini-css-extract-plugin": "^0.8.0",
    "node-sass": "^7.0.0",
    "optimize-css-assets-webpack-plugin": "^5.0.3",
    "popper.js": "^1.16.1",
    "postcss-loader": "^3.0.0",
    "sass-loader": "^8.0.0",
    "style-loader": "^1.0.0",
    "terser-webpack-plugin": "^3.0.2",
    "uglifyjs-webpack-plugin": "^2.2.0",
    "webpack": "^4.41.5",
    "webpack-cli": "^3.3.9",
    "webpack-dev-server": "^3.11.0"
  },
  "dependencies": {
    "@babel/polyfill": "^7.8.3",
    "autoprefixer": "^9.7.4",
    "file-loader": "^6.0.0",
    "gh-pages": "^2.2.0",
    "normalize.css": "^8.0.1"
  }
}
